// This is your Prisma schema file,
// learn more reviews it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Reviews {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  nameReview String
  descReview String
}
